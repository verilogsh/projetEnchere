@model IEnumerable<Enchere.Models.SyntheseVentes>

@{
    ViewBag.Title = "SyntheseVenteAnnuel";
}

<h2>Synthèse Ventes Annuelles</h2>

<p>
    @Html.ActionLink("Imprimer la synthese annuelle", "PrintSyntheseVenteAnnuel")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomObjet)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Prix)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Commision)
        </th>
      
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomObjet)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Prix)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Commision)
        </td>
      
     
    </tr>
}

    <tr>
      
        <td>
           <b>Total Ventes</b>
        </td>
        <td>
           @ViewBag.TotalVente
        </td>
        <td>
            <b>Total Commission</b>
        </td>
        <td>
            @ViewBag.TotalCom
        </td>

    </tr>

</table>
