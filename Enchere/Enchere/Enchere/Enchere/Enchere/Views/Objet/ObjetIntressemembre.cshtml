@model IEnumerable<Enchere.Models.HistoEncheObjet>
@using Enchere.Ressource
@{
    ViewBag.Title = "ObjetIntressemembre";
}

<h2>Objets qui m'ont intéressés</h2>

<p>
    @Html.ActionLink(@ResourceView.Imprimerrapport, "PrintObjetIntressemembre", "Objet")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdEnchere)
        </th>
        <th>
            @ResourceView.nom2
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @ResourceView.Catégorie
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Photo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Piece)
        </th>
        <th>
            @ResourceView.Vendeur
        </th>
        <th>
            @ResourceView.Acheteur
        </th>
        <th>
            @ResourceView.PrixDepart
        </th>
        <th>
            @ResourceView.PrixActuel
        </th>
        <th>
            @ResourceView.DateDepart
        </th>
        <th>
            @ResourceView.DateFin
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Etat)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdEnchere)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCategorie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Photo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Piece)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdVendeur)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAcheteur)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrixDepart)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrixActuel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateDepart)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateFin)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Etat)
            </td>

        </tr>
    }

</table>
